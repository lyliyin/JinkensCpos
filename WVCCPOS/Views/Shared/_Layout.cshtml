<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title> 经销商CRM管理平台 </title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="小银子" name="description" />
    <meta content="小银子" name="author" />
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="fixed-top">
    <!-- BEGIN HEADER -->
    @Html.Partial("Header")
    <!-- END HEADER -->
    <!-- BEGIN CONTAINER -->
    <div id="container" class="row-fluid">
        <!-- BEGIN SIDEBAR -->
        @Html.Partial("Menu")
        <!-- END SIDEBAR -->
        <!-- BEGIN PAGE -->
        <div id="main-content">
            <!-- BEGIN PAGE CONTAINER-->
            @RenderBody()
            <!-- END PAGE CONTAINER-->
        </div>
        <!-- END PAGE -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    @Html.Partial("Footer")

    @Scripts.Render("~/Scripts/bootstrap")
    @RenderSection("scripts", required: false)

    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS -->
    <!-- Load javascripts at bottom, this will reduce page load time -->
    <!-- ie8 fixes -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Content/js/excanvas.js")"></script>
    <script src="@Url.Content("~/Content/js/respond.js")"></script>
    <![endif]-->
    <!-- ie8 fixes -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Content/js/excanvas.js")"></script>
    <script src="@Url.Content("~/Content/js/respond.js")"></script>
    <![endif]-->
    <script type="text/javascript">
        jQuery(document).ready(function () {
            //App.setMainPage(true);
            App.init();
        });
    </script>
    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>
